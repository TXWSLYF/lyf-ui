{
  "name": "lyf-ui",
  "version": "0.0.1",
  "description": "lyf 的 vue 组件库",
  "main": "lib/lyf-ui.min.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "clean": "rimraf lib",
    "build:style": "npm run clean && gulp --gulpfile build/gen-style.js",
    "build:components": "webpack --config build/webpack.component.js",
    "build:prod": "webpack --config build/webpack.prod.js",
    "dist": "npm run test && npm run clean && npm run build:style && npm run build:prod && npm run build:components",
    "test": "karma start test/karma.config.js --single-run"
  },
  "author": "alex",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "core-js": "2.6.9",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.0",
    "node-sass": "^4.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vuepress": "^1.3.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "browserslist": [
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "Safari >= 10",
    "Explorer >= 11",
    "Edge >= 12",
    "iOS >= 10",
    "Android >= 6"
  ]
}
